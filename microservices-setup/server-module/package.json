{
  "name": "@orgName/server",
  "version": "0.1.0",
  "description": "Server application for orgName",
  "main": "server.ts",
  "scripts": {
    "dev": "nodemon --exec npx ts-node server.ts",
    "generate-npm": "node ./shell/generate_npm",
    "lint": "eslint --ext .ts"
  },
  "private": true,
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "body-parser": "^1.19.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "custom-env": "^2.0.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.8.0",
    "express": "^4.17.2",
    "helmet": "^5.0.2",
    "hpp": "^0.2.3",
    "https": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "node": "^17.4.0",
    "node-cache": "^5.1.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/hpp": "^0.2.2",
    "@types/node": "^17.0.17",
    "nodemon": "^2.0.15",
    "shelljs": "^0.8.5",
    "snyk": "^1.852.0",
    "ts-node": "^10.5.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5"
  },
  "vsc-organize-imports": {
    "orderSpecifiers": true,
    "orderSpecifiersAsSingleLine": true,
    "baseUrl": "src",
    "emptyLinesAfterImports": 1,
    "emptyLinesBetweenFilledGroups": 1,
    "groups": [
      {
        "groups": [
          "global"
        ],
        "sortBy": "path",
        "emptyLines": true
      },
      {
        "groups": [
          "absolute"
        ],
        "sortBy": "path",
        "emptyLines": true
      },
      {
        "groups": [
          "relative"
        ],
        "sortBy": "path",
        "emptyLines": true
      },
      {
        "groups": [
          "globalDirect",
          "absoluteDirect",
          "relativeDirect"
        ],
        "sortBy": "path",
        "emptyLines": true
      }
    ]
  }
}
